// https://eslint.org/docs/rules/

{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "rules": {
    "no-console": "warn",
    "no-unused-vars": ["error", { "argsIgnorePattern": "req|res|next|val" }]
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "browser": true
  }
}
